!-------------------------------------------------------------------------------
!
!	U R X V T - Settings
!
URxvt*geometry:					112x33
URxvt*foreground:				white
URxvt*borderLess:				false
URxvt*depth:					32
URxvt*background:				rgba:0000/0000/0200/c800
URxvt*scrollBar:				false
URxvt.font:					xft:xos4 Terminus:size=9
URxvt.iconFile:                                 /usr/share/icons/mate/256x256/apps/terminal.png
URxvt*cursorBlink:				false
URxvt*cursorUnderline:				false
URxvt*scrollColor:				#111111
URxvt*perl-ext-common:				default,matcher,clipboard,fullscreen,vtwheel
URxvt*matcher.button:				1
URxvt*matcher:					1
URxvt*url-launcher:                              /usr/bin/firefox
URxvt*keysym.F11: 				perl:fullscreen:switch

